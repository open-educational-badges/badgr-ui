{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"web-components-cli": {
			"projectType": "application",
			"root": ".",
			"sourceRoot": "src",
			"architect": {
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": ["webcomponents/**/*.ts", "webcomponents/**/*.html"]
					}
				},
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"index": "webcomponents/index.html",
						"tsConfig": "tsconfig.wc.json",
						"polyfills": ["src/polyfills.ts"],
            "inlineStyleLanguage": "scss",
            "stylePreprocessorOptions": {
              "includePaths": [
                "."
              ]
            }
					},
					"configurations": {
            "oeb-version": {
              "outputPath": {
                "base": "dist/webcomponents/oeb-version"
              },
              "browser": "src/app/public/components/version.web-component.ts"
						},
            "oeb-skill-visualisation": {
              "outputPath": {
                "base": "dist/webcomponents/oeb-skill-visualisation"
              },
							"styles": [
								"src/styles/oeb/oeb_styles.scss",
								"src/styles/google-webfonts/nunito.scss",
								"src/styles/google-webfonts/rubik.scss",
								"src/app/recipient/components/recipient-skill-visualisation/recipient-skill-visualisation.component.scss"
							],
              "assets": [
                "src/favicon.ico",
                "src/assets/@concentricsky/badgr-style/dist/images/icons.svg"
              ],
              "browser": "src/app/recipient/components/recipient-skill-visualisation/recipient-skill-visualisation.web-component.ts"
						},
            "oeb-earned-badges-overview": {
              "outputPath": {
                "base": "dist/webcomponents/oeb-earned-badges-overview"
              },
							"styles": [
								"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
								"src/styles/oeb/oeb_styles.scss",
								"src/styles/google-webfonts/nunito.scss",
								"src/styles/google-webfonts/rubik.scss"
							],
							"assets": [
								"src/favicon.ico",
								"src/assets/@concentricsky/badgr-style/dist/images/icons.svg",
								"src/breakdown/static/images/badge-loading.svg",
								"src/breakdown/static/images/badge-failed.svg",
								"src/assets/@concentricsky/badgr-style/dist/images/image-empty-backpack.svg"
              ],
              "browser": "src/app/recipient/components/recipient-earned-badges-overview/recipient-earned-badges-overview.web-component.ts"
						},
            "oeb-competency-overview": {
              "outputPath": {
                "base": "dist/webcomponents/oeb-competency-overview"
              },
							"styles": [
								"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
								"src/styles/oeb/oeb_styles.scss",
								"src/styles/google-webfonts/nunito.scss",
								"src/styles/google-webfonts/rubik.scss"
							],
              "assets": [
                "src/favicon.ico",
                "src/assets/@concentricsky/badgr-style/dist/images/icons.svg"
              ],
              "browser": "src/app/recipient/components/recipient-competency-overview/recipient-competency-overview.web-component.ts"
						},
            "oeb-learningpaths-overview": {
              "outputPath": {
                "base": "dist/webcomponents/oeb-learningpaths-overview"
              },
							"styles": [
								"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
								"src/styles/oeb/oeb_styles.scss",
								"src/styles/google-webfonts/nunito.scss",
								"src/styles/google-webfonts/rubik.scss"
							],
							"assets": [
								"src/favicon.ico",
								"src/assets/@concentricsky/badgr-style/dist/images/icons.svg",
								"src/breakdown/static/images/badge-loading.svg",
								"src/breakdown/static/images/badge-failed.svg",
								"src/assets/oeb/images/learningPath/learningPathIcon.svg",
								"src/assets/@concentricsky/badgr-style/dist/images/image-empty-backpack.svg"
              ],
              "browser": "src/app/recipient/components/recipient-learningpaths-overview/recipient-learningpaths-overview.web-component.ts"
						},
            "oeb-backpack": {
              "outputPath": {
                "base": "dist/webcomponents/oeb-backpack"
              },
							"styles": [
								"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
								"src/styles/oeb/oeb_styles.scss",
								"src/styles/google-webfonts/nunito.scss",
								"src/styles/google-webfonts/rubik.scss",
								"src/app/recipient/components/recipient-skill-visualisation/recipient-skill-visualisation.component.scss"
							],
							"assets": [
								"src/favicon.ico",
								"src/assets/@concentricsky/badgr-style/dist/images/icons.svg",
								"src/breakdown/static/images/badge-loading.svg",
								"src/breakdown/static/images/badge-failed.svg",
								"src/assets/oeb/images/learningPath/learningPathIcon.svg",
								"src/assets/@concentricsky/badgr-style/dist/images/image-empty-backpack.svg"
              ],
              "browser": "src/app/recipient/components/recipient-backpack/recipient-backpack.web-component.ts"
						},
            "oeb-badge-creation": {
              "outputPath": {
                "base": "dist/webcomponents/oeb-badgeclass-edit-form"
              },
							"styles": [
								"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
								"src/styles/oeb/oeb_styles.scss",
								"src/styles/google-webfonts/nunito.scss",
								"src/styles/google-webfonts/rubik.scss",
								"src/app/issuer/components/badgeclass-edit-form/badgeclass-edit-form.component.css",
								"src/app/issuer/components/badgeclass-select-type/badgeclass-select-type.component.scss"
							],
							"assets": [
								"src/favicon.ico",
								"src/assets/oeb/images/badge/icon-ai-skills.svg",
								"src/assets/oeb/images/badge/loading.svg",
								"src/assets/oeb/images/badge/icon-ai-betterresults.svg",
								"src/breakdown/static/images/square.svg",
								"src/breakdown/static/badgestudio/shapes/participation.svg",
								"src/breakdown/static/badgestudio/shapes/competency.svg",
								"src/breakdown/static/badgestudio/shapes/learningpath.svg",
								"src/breakdown/static/images/badge-loading.svg",
								"src/breakdown/static/images/badge-failed.svg"
              ],
              "browser": "src/app/issuer/components/badgeclass-edit-form/badgeclass-edit-form.web-component.ts"
						},
						"production": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							]
						},
						"staging": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.staging.ts"
								}
							]
						},
						"develop": {
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.develop.ts"
								}
							]
						},
						"local": {}
					}
				}
			}
		},
		"badgr-ui-cli": {
			"root": ".",
			"sourceRoot": "src",
			"projectType": "application",
			"architect": {
				"lint": {
					"builder": "@angular-eslint/builder:lint",
					"options": {
						"lintFilePatterns": ["src/**/*.ts", "src/**/*.html"]
					}
				},
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"outputPath": {
							"base": "dist",
							"browser": ""
						},
						"index": "src/index.html",
						"tsConfig": "tsconfig.app.json",
						"polyfills": ["src/polyfills.ts"],
						"assets": [
							"src/assets",
							"src/breakdown",
							"src/_redirects",
							{
								"glob": "**/*",
								"input": "node_modules/ace-builds/src-min",
								"output": "./assets/ace-builds/"
							},
							{
								"glob": "**/*",
								"input": "./assets/@concentricsky/badgr-style/dist/images/",
								"output": "./assets/@concentricsky/badgr-style/dist/images/"
							}
						],
						"styles": [
							"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
							"src/styles/oeb/oeb_styles.scss",
							"src/styles/google-webfonts/nunito.scss",
							"src/styles/google-webfonts/rubik.scss",
							"node_modules/dialog-polyfill/dialog-polyfill.css",
							"src/breakdown/static/css/re-style.css",
							"node_modules/maplibre-gl/dist/maplibre-gl.css",
							"node_modules/ngx-markdown-editor/assets/highlight.js/agate.min.css"
						],
						"scripts": [
							"node_modules/ngx-markdown-editor/assets/highlight.js/highlight.min.js",
							"node_modules/ngx-markdown-editor/assets/marked.min.js",
							"node_modules/altcha/dist/altcha.js"
						],
						"extractLicenses": false,
						"sourceMap": true,
						"optimization": false,
						"namedChunks": true,
						"inlineStyleLanguage": "scss",
						"browser": "src/main.ts"
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"index": {
								"input": "src/index.production.html",
								"output": "index.html"
							},
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								}
							]
						},
						"staging": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"index": {
								"input": "src/index.staging.html",
								"output": "index.html"
							},
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.staging.ts"
								}
							]
						},
						"develop": {
							"budgets": [
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"index": {
								"input": "src/index.develop.html",
								"output": "index.html"
							},
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.develop.ts"
								}
							]
						}
					},
					"defaultConfiguration": ""
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"options": {
						"buildTarget": "badgr-ui-cli:build"
					},
					"configurations": {
						"production": {
							"buildTarget": "badgr-ui-cli:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular/build:extract-i18n",
					"options": {
						"buildTarget": "badgr-ui-cli:build"
					}
				},
				"test": {
					"builder": "@angular/build:karma",
					"options": {
						"polyfills": ["zone.js", "zone.js/testing"],
						"tsConfig": "tsconfig.spec.json",
						"karmaConfig": "src/karma.conf.js",
						"styles": [
							"src/assets/@concentricsky/badgr-style/dist/css/screen.css",
							"node_modules/dialog-polyfill/dialog-polyfill.css"
						],
						"scripts": [
							"node_modules/ngx-markdown-editor/assets/marked.min.js",
							"node_modules/ace-builds/src-min/ace.js"
						],
						"assets": [
							"src/favicon.ico",
							"src/assets",
							{
								"glob": "**/*",
								"input": "node_modules/ace-builds/src-min",
								"output": "./assets/ace-builds/"
							},
							{
								"glob": "icons.svg",
								"input": "./assets/@concentricsky/badgr-style/dist/images",
								"output": "./assets/@concentricsky/badgr-style/dist/images"
							}
						],
						"inlineStyleLanguage": "scss"
					}
				}
			}
		}
	},
	"schematics": {
		"@schematics/angular:component": {
			"prefix": "app",
			"style": "css",
			"type": "component"
		},
		"@schematics/angular:directive": {
			"prefix": "app",
			"type": "directive"
		},
		"@schematics/angular:service": {
			"type": "service"
		},
		"@schematics/angular:guard": {
			"typeSeparator": "."
		},
		"@schematics/angular:interceptor": {
			"typeSeparator": "."
		},
		"@schematics/angular:module": {
			"typeSeparator": "."
		},
		"@schematics/angular:pipe": {
			"typeSeparator": "."
		},
		"@schematics/angular:resolver": {
			"typeSeparator": "."
		}
	},
	"cli": {
		"analytics": false
	}
}
